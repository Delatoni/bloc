{
   "background": {
      "persistent": false,
      "scripts": [ "assets/bloc.js" ]
   },
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "content_security_policy": "script-src 'self' https://www.google-analytics.com https://query.yahooapis.com 'unsafe-eval'; object-src 'self'",
   "description": "Replace new tab page with a personal dashboard featuring todo, weather, and inspiration.",
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
   },
   "manifest_version": 2,
   "name": "Bloc",
   "offline_enabled": true,
   "permissions": [ "geolocation", "unlimitedStorage", "*://*.yahooapis.com/*", "*://*.momentumdash.com/*" ],
   "short_name": "Bloc",
   "version": "1.0"
}
